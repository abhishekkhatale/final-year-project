{
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      { "source": "/dashboard", "destination": "/index.html" },
      { "source": "/login", "destination": "/index.html" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
    
  }